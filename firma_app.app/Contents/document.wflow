					<string>#!/bin/bash

# Ottieni il percorso dell'app Automator (cio√® questo script .app)
AUTOMATOR_PATH="$(dirname "$0")"

# Trova tutte le app .app nella stessa cartella
APP_LIST=$(find "$AUTOMATOR_PATH" -maxdepth 1 -name "*.app")

for APP_PATH in $APP_LIST; do
    echo "üîè Firmo: $APP_PATH"
    /usr/bin/codesign --force --deep --sign - "$APP_PATH"
    /usr/bin/xattr -d com.apple.quarantine "$APP_PATH"
done

/usr/bin/osascript -e 'display notification "Tutte le app sono state firmate." with title "Firma App"'
