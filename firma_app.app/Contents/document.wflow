					<key>COMMAND_STRING</key>
					<string>#!/bin/bash

APP_PATH=~/Desktop/mac-arm64/scrapantonio.app

echo "üîç Verifica firma prima di firmare: $APP_PATH"
if codesign --verify --deep --strict --verbose=2 "$APP_PATH" 2&gt;&amp;1 | grep -q "valid on disk"; then
  echo "‚úÖ L'app √® gi√† firmata correttamente."
else
  echo "‚ö†Ô∏è L'app NON √® firmata o la firma √® invalida."
fi

if xattr "$APP_PATH" 2&gt;/dev/null | grep -q "com.apple.quarantine"; then
  echo "‚ö†Ô∏è L'app √® soggetta a quarantena (bloccata da macOS)."
else
  echo "‚úÖ L'app NON √® soggetta a quarantena."
fi

echo "üîè Firmo l'app..."
codesign --force --deep --sign - "$APP_PATH"

echo "üö´ Rimuovo quarantena..."
xattr -d com.apple.quarantine "$APP_PATH" 2&gt;/dev/null || true

echo "üîç Verifica firma dopo la firma: $APP_PATH"
if codesign --verify --deep --strict --verbose=2 "$APP_PATH" 2&gt;&amp;1 | grep -q "valid on disk"; then
  echo "‚úÖ Firma valida dopo l'operazione."
else
  echo "‚ùå Firma NON valida dopo l'operazione!"
fi

echo "üîç Controllo quarantena dopo la firma: $APP_PATH"
if xattr "$APP_PATH" 2&gt;/dev/null | grep -q "com.apple.quarantine"; then
  echo "‚ùå L'app √® ancora soggetta a quarantena!"
else
  echo "‚úÖ Quarantena rimossa con successo."
fi

echo "Fatto!"

</string>
